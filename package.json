{
	"name": "go-to-next-problem",
	"displayName": "Go to Next Problem",
	"description": "Resolves https://github.com/microsoft/vscode/issues/105795",
	"version": "0.1.0",
	"publisher": "JimmyZJX",
	"icon": "icon.gif",
	"engines": {
		"vscode": "^1.57.0"
	},
	"extensionKind": [
		"ui",
		"workspace"
	],
	"categories": [
		"Other"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/JimmyZJX/go-to-next-problem"
	},
	"bugs": {
		"url": "https://github.com/JimmyZJX/go-to-next-problem/issues"
	},
	"activationEvents": [ ],
	"keywords": [
		"error",
		"warning",
		"problem",
		"marker"
	],
	"main": "./out/extension.js",
	"browser": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "go-to-next-problem.next",
				"title": "Go to Next Problem (Error)"
			},
			{
				"command": "go-to-next-problem.prev",
				"title": "Go to Previous Problem (Error)"
			},
			{
				"command": "go-to-next-problem.nextInFiles",
				"title": "Go to Next Problem in Files (Error)"
			},
			{
				"command": "go-to-next-problem.prevInFiles",
				"title": "Go to Previous Problem in Files (Error)"
			}
		]
	},
	"capabilities": {
		"virtualWorkspaces": true,
		"untrustedWorkspaces": {
			"supported": true
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"vsce": "vsce package --out goToNextProblem.vsix",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./"
	},
	"devDependencies": {
		"@types/node": "^17.0.33",
		"@types/vscode": "^1.57.0",
		"typescript": "^5.0.4"
	}
}
